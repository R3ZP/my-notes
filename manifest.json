{
  "manifest_version": 2,
  "name": "My Notes",
  "description": "My Notes â€” Chrome extension for note-taking in a New Tab.",
  "version": "3.0.1",
  "homepage_url": "https://github.com/penge/my-notes",
  "icons": { "128": "images/icon128.png" },
  "options_page": "options.html",
  "permissions": ["storage", "contextMenus"],
  "optional_permissions": ["tabs", "identity"],
  "oauth2": {
    "client_id": "813847979218-unbbh8m0el8vjdi74rnqdarm1mhv3985.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file"
    ]
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon128.png"
  },
  "commands": {
    "focus": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Alt+F"
      },
      "description": "Toggle Focus mode"
    }
  },
  "minimum_chrome_version": "61",
  "offline_enabled": true
}
